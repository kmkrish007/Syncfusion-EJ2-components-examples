@page "/10000del"


@using Syncfusion.Blazor.Buttons

<SfChip EnableDelete="true">
    <ChipItems>
        @foreach (var item in Items)
        {
            <ChipItem Text="@item.Name"></ChipItem>
        }
    </ChipItems>
</SfChip>

@code{

    public class ChipData
    {
        public string Name { get; set; }
    }
    public List<ChipData> Items { get; set; }
    protected override void OnInitialized()
    {
        Items = Enumerable.Range(1, 10000).Select(x => new ChipData()
        {
            Name = "Item" + x,
        }).ToList();
    }

}